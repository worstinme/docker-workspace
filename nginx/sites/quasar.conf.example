server {
  listen 80;
  server_name default;

  location / {
      proxy_pass http://quasar:3000;
      proxy_set_header Host $host;
      proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
  }

  error_log /var/log/nginx/frontend_error.log;
  access_log /var/log/nginx/frontend_access.log;
}
